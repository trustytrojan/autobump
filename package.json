{
	"dependencies": {
		"discord.js-selfbot-v13": "^3.1.4",
		"dotenv": "^16.4.5"
	},
	"devDependencies": {
		"@types/node": "^22.13.5",
		"prettier": "^3.5.2"
	},
	"type": "module",
	"scripts": {
		"start": "bash -c 'deno -A src/index.ts &>log & echo $! >pid'",
