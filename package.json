{
	"dependencies": {
		"@types/node": "^22.10.5",
		"discord.js-selfbot-v13": "^3.1.4",
		"dotenv": "^16.4.5",
		"prettier": "^3.4.2",
		"typescript": "^5.7.2"
	},
	"type": "module",
	"scripts": {
		"start": "bash -c 'node . &>log & echo $! >pid'",
		"stop": "bash -c 'kill $(<pid)'",
		"fmt": "prettier --log-level=warn --write ./src/**/*.ts",
		"build": "tsc"
	},
	"prettier": {
		"arrowParens": "avoid",
		"trailingComma": "none",
		"useTabs": true,
		"printWidth": 120,
		"tabWidth": 4,
		"semi": true,
		"singleQuote": true,
		"proseWrap": "preserve"
	}
}
